<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="10.0" layoutY="14.0" text="Xor Size">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button fx:id="save" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#save" prefHeight="40.0" prefWidth="285.0" text="Save" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" />
      <Button fx:id="cancel" layoutX="544.0" layoutY="371.0" mnemonicParsing="false" onAction="#cancel" prefHeight="40.0" prefWidth="285.0" text="Cancel" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" />
      <Label layoutX="10.0" layoutY="70.0" text="Input Sizes">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TableView fx:id="inputTable" layoutX="10.0" layoutY="90.0" prefHeight="150.0" prefWidth="285.0">
        <columns>
          <TableColumn fx:id="InputIds" prefWidth="145.0" text="Input Link IDs" />
          <TableColumn fx:id="InputIdSizes" prefWidth="138.0" text="Size" />
        </columns>
      </TableView>
      <TableView fx:id="outputTable" layoutX="305.0" layoutY="90.0" prefHeight="150.0" prefWidth="285.0" AnchorPane.rightAnchor="0.0">
         <columns>
            <TableColumn fx:id="OutputIds" prefWidth="145.0" text="Output Link IDs" />
            <TableColumn fx:id="OutputIdSizes" prefWidth="138.0" text="Size" />
         </columns>
      </TableView>
      <Label layoutX="305.0" layoutY="70.0" text="Output Sizes">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField layoutX="10.0" layoutY="34.0" fx:id="xorSize" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="10.0" layoutY="14.0" text="LookUp Table">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="285.0" text="Save" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" />
      <Button layoutX="544.0" layoutY="371.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="285.0" text="Cancel" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" />
      <TextArea fx:id="LookupText" layoutX="10.0" layoutY="36.0" prefHeight="110.0" prefWidth="320.0" promptText="Enter lookup table values here seperated by a space for each value or load in lookup values from file&#10;" />
      <Button layoutX="387.0" layoutY="41.0" mnemonicParsing="false" onAction="#LoadLookupFromFile" text="Choose File">
         <tooltip>
            <Tooltip text="Choose text file witrh lookup table to load in" />
         </tooltip></Button>
      <Label layoutX="354.0" layoutY="45.0" text="or" />
      <Label layoutX="10.0" layoutY="163.0" text="Input Sizes">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TableView fx:id="inputTable" layoutX="10.0" layoutY="185.0" prefHeight="150.0" prefWidth="285.0">
        <columns>
          <TableColumn fx:id="InputIds" prefWidth="145.0" text="Input Link IDs" />
          <TableColumn fx:id="InputIdSizes" prefWidth="138.0" text="Size" />
        </columns>
      </TableView>
      <TableView fx:id="outputTable" layoutX="305.0" layoutY="185.0" prefHeight="150.0" prefWidth="285.0" AnchorPane.rightAnchor="0.0">
         <columns>
            <TableColumn fx:id="OutputIds" prefWidth="145.0" text="Output Link IDs" />
            <TableColumn fx:id="OutputIdSizes" prefWidth="138.0" text="Size" />
         </columns>
      </TableView>
      <Label layoutX="305.0" layoutY="163.0" text="Output Sizes">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</AnchorPane>
